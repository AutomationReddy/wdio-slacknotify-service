{
  "name": "wdio-slacknotify-service",
  "version": "1.0.0",
  "description": "Webdriverio library to send test results as a slack notification/message to channels",
  "main": "src/index.js",
  "scripts": {
    "commit": "cz",
    "release": "standard-version",
    "release:minor": "standard-version --release-as minor",
    "release:patch": "standard-version --release-as patch",
    "release:major": "standard-version --release-as major"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AutomationReddy/wdio-slacknotify-service.git"
  },
  "keywords": [
    "webdriverio",
    "wdio",
    "wdio-service",
    "wdio-plugin",
    "slack"
  ],
  "author": "Vinod Reddy <vinodkumarv8197@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/AutomationReddy/wdio-slacknotify-service/issues"
  },
  "homepage": "https://github.com/AutomationReddy/wdio-slacknotify-service#readme",
  "contributors": [
    "Vinod Reddy <vinodkumarv8197@gmail.com>"
  ],
  "dependencies": {
    "@commitlint/cli": "^12.1.1",
    "@slack/webhook": "^6.0.0",
    "@wdio/cli": "^7.5.7"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^12.1.1",
    "cz-conventional-changelog": "^3.3.0",
    "husky": "^6.0.0",
    "standard-version": "^9.3.0"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  }
}
